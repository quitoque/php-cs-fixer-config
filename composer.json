{
    "name": "quitoque/php-cs-fixer-config",
    "description": "Configuration for PhpCsFixer used by Quitoque",
    "keywords": [
        "PhpCsFixer",
        "Quitoque",
        "Coding style"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "RÃ©mi Chomat",
            "email": "remi@quitoque.fr"
        }
    ],
    "require": {
        "friendsofphp/php-cs-fixer": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Quitoque\\Tools\\PhpCsFixer\\": "src"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
